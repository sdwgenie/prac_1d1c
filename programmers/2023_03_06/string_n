def solution(my_string, n):
    answer = ''
    for i in range(len(my_string)) :
        for _ in range(n) :
            answer += my_string[i]
    
    return answer
    
def solution2(my_string, n):
    answer = ''

    for c in list(my_string):
        answer += c*n
    return answer

def solution(my_string, n):
    return ''.join(i*n for i in my_string)
